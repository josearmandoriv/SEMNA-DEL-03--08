#include <math.h>
/* numemp: compara s i y s2 num√©ricamente */
int numcmp(char *sl, char *s2)
{
double v1, v2;
v1 = atof(sl);
v2 = atof(s2);
if (v1 < v2)
return -1;
else 
if (v1 > v2)
return 1;
else
return 0;
}
void swap(void *v[ ] , int i, int j)
{
void *temp;
temp = v[i];
v[i] = v[j];
v[j] = temp;
}
